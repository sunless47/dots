#!/usr/bin/env bash
#============#
# api tester #
#============#
 
# error handling
set -euo pipefail

# test api
quick-api() {
  # target link
  link=""

  # using curl to test
  curl \
    "${link}"
}

# test api with different requests
crud-api() {
  # target link
  link=""

  # using curl to test
  curl \
    --header "Content-Type: application/json" \
    --request POST \
    --json '{
      "": ,
      "": "",
      "":
    }' \
    "${link}"
}

# calling function
#quick-api
#crud-api

# hack to go to the next line
echo ""
